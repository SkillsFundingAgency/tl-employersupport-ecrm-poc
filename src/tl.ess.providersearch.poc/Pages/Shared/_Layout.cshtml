<script src="~/lib/crypto-js/core.js"></script>
<script src="~/lib/crypto-js/enc-base64.js"></script>
<script src="~/lib/crypto-js/enc-utf8.js"></script>
<script src="~/lib/crypto-js/sha256.js"></script>
<script src="~/lib/crypto-js/hmac.js"></script>
<script src="~/lib/crypto-js/hmac-sha256.js"></script>

	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>

	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
